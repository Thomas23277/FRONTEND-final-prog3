{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "noEmitOnError": false,
    "outDir": "dist",
    "types": ["vite/client"],
    "noImplicitAny": false

  },
  "include": ["src", "vite.config.ts", "src/env.d.ts"]
}
